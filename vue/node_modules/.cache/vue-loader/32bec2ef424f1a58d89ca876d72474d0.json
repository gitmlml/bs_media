ÊóÖÊ∏∏ÂçöÂÆ¢Á≥ªÁªü</b></div>\r\n      <el-form :model=\"user\" :rules=\"rules\" ref=\"userForm\">\r\n\r\n        <el-form-item prop=\"loginType\">\r\n          <el-select style=\"width: 320px\" v-model=\"user.loginType\" placeholder=\"‚ú®ËØ∑ÈÄâÊã©ÁôªÂΩïÊñπÂºè\">\r\n            <el-option label=\"üåêË¥¶Âè∑\" value=\"account\"></el-option>\r\n            <el-option label=\"üì≥ÊâãÊú∫Âè∑\" value=\"phone\"></el-option>\r\n            <!-- <el-option label=\"üíåÈ™åËØÅÁ†Å\" value=\"emailCode\"></el-option> -->\r\n          </el-select>\r\n        </el-form-item>\r\n\r\n        <!-- Ê†πÊçÆÁôªÂΩïÁ±ªÂûãÊòæÁ§∫‰∏çÂêåÁöÑË°®Âçï -->\r\n        <el-form-item v-if=\"user.loginType === 'account'\">\r\n          <el-form-item prop=\"username\">\r\n            <el-input size=\"medium\" prefix-icon=\"el-icon-user\" placeholder=\"ËØ∑ËæìÂÖ•Ë¥¶Âè∑\" v-model=\"user.username\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item prop=\"password\">\r\n            <el-input size=\"medium\" prefix-icon=\"el-icon-lock\" placeholder=\"ËØ∑ËæìÂÖ•ÂØÜÁ†Å\" show-password v-model=\"user.password\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item>\r\n            <div style=\"display: flex\">\r\n              <el-input placeholder=\"ËØ∑ËæìÂÖ•È™åËØÅÁ†Å\" size=\"mid\" v-model=\"code\" style=\"width: 200px\"></el-input>\r\n              <span @click=\"refreshCode\" style=\"cursor: pointer; flex: 1;\">\r\n              <Identify :identifyCode=\"identifyCode\"></Identify>\r\n           </span>\r\n            </div>\r\n          </el-form-item>\r\n          <el-form-item style=\"margin: 10px 0; text-align: center\">\r\n            <el-button style=\"width: 130px\" type=\"success\" size=\"medium\"  autocomplete=\"off\" @click=\"login\">ÁôªÂΩï</el-button>\r\n            <el-button type=\"text\" size=\"medium\" style=\"background-color: #76de97; font-size: 16px; width: 130px; color: white\" autocomplete=\"off\" @click=\"$router.push('/register')\">Ê≥®ÂÜå</el-button>\r\n\r\n          </el-form-item>\r\n          <el-form-item style=\"margin: 10px 0; text-align: right\">\r\n            <el-button type=\"text\" size=\"medium\"  autocomplete=\"off\" @click=\"handlePass\">ÂøòËÆ∞ÂØÜÁ†Å</el-button>\r\n          </el-form-item>\r\n        </el-form-item>\r\n\r\n        <el-form-item v-else-if=\"user.loginType === 'phone'\">\r\n          <el-form-item prop=\"phone\">\r\n            <el-input size=\"medium\" prefix-icon=\"el-icon-phone\" placeholder=\"ËØ∑ËæìÂÖ•ÊâãÊú∫Âè∑Á†Å\" v-model=\"user.phone\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item prop=\"password\">\r\n            <el-input size=\"medium\" prefix-icon=\"el-icon-lock\" placeholder=\"ËØ∑ËæìÂÖ•ÂØÜÁ†Å\" show-password v-model=\"user.password\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item>\r\n            <div style=\"display: flex\">\r\n              <el-input placeholder=\"ËØ∑ËæìÂÖ•È™åËØÅÁ†Å\" size=\"mid\" v-model=\"code\" style=\"width: 200px\"></el-input>\r\n              <span @click=\"refreshCode\" style=\"cursor: pointer; flex: 1;\">\r\n              <Identify :identifyCode=\"identifyCode\"></Identify>\r\n           </span>\r\n            </div>\r\n          </el-form-item>\r\n          <el-form-item style=\"margin: 10px 0; text-align: center\">\r\n            <el-button style=\"width: 130px\" type=\"success\" size=\"medium\"  autocomplete=\"off\" @click=\"loginPhone\">ÁôªÂΩï</el-button>\r\n            <el-button type=\"text\" size=\"medium\" style=\"background-color: #76de97; font-size: 16px; width: 130px; color: white\" autocomplete=\"off\" @click=\"$router.push('/register')\">Ê≥®ÂÜå</el-button>\r\n\r\n          </el-form-item>\r\n          <el-form-item style=\"margin: 10px 0; text-align: right\">\r\n            <el-button type=\"text\" size=\"medium\"  autocomplete=\"off\" @click=\"handlePass\">ÂøòËÆ∞ÂØÜÁ†Å</el-button>\r\n          </el-form-item>\r\n        </el-form-item>\r\n\r\n      </el-form>\r\n    </div>\r\n\r\n\r\n    <el-dialog title=\"ÊâæÂõûÂØÜÁ†Å\" :visible.sync=\"dialogFormVisible\" width=\"30%\" >\r\n      <el-form label-width=\"100px\" size=\"small\">\r\n        <el-form-item label=\"Áî®Êà∑Âêç\">\r\n          <el-input v-model=\"pass.username\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"ÊâãÊú∫Âè∑\">\r\n          <el-input v-model=\"pass.phone\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">Âèñ Ê∂à</el-button>\r\n        <el-button type=\"primary\" @click=\"passwordBack\">ÈáçÁΩÆÂØÜÁ†Å</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {resetRouter, setRoutes} from \"@/router\";\r\nimport Identify from \"@/components/Identify\";\r\n\r\nexport default {\r\n  name: \"Login\",\r\n  data() {\r\n    return {\r\n      loginType: 'account',  // ÈªòËÆ§ÈÄâÊã©Ë¥¶Âè∑ÁôªÂΩï\r\n      user: {loginType: 'account'}, // ÂàùÂßãÂåñÊó∂ËÆæÁΩÆ‰∏∫Ë¥¶Âè∑ÁôªÂΩï\r\n      pass: {},\r\n      code: '',\r\n      dialogFormVisible: false,\r\n      // ÂõæÁâáÈ™åËØÅÁ†Å\r\n      identifyCode: '',\r\n      // È™åËØÅÁ†ÅËßÑÂàô\r\n      identifyCodes: '3456789ABCDEFGHGKMNPQRSTUVWXY',\r\n      rules: {\r\n        username: [\r\n          { required: true, message: 'ËØ∑ËæìÂÖ•Áî®Êà∑Âêç', trigger: 'blur' },\r\n          { min: 3, max: 10, message: 'ÈïøÂ∫¶Âú® 3 Âà∞ 10 ‰∏™Â≠óÁ¨¶', trigger: 'blur' }\r\n        ],\r\n        password: [\r\n          { required: true, message: 'ËØ∑ËæìÂÖ•ÂØÜÁ†Å', trigger: 'blur' },\r\n          { min: 1, max: 20, message: 'ÈïøÂ∫¶Âú® 1 Âà∞ 20 ‰∏™Â≠óÁ¨¶', trigger: 'blur' }\r\n        ],\r\n      }\r\n    }\r\n  },\r\n  components: {Identify},\r\n  mounted() {\r\n    // ÈáçÁΩÆË∑ØÁî±\r\n    resetRouter()\r\n    this.refreshCode()\r\n  },\r\n\r\n  methods: {\r\n    login() {\r\n      if (!this.code) {\r\n        this.$message.warning('ËØ∑ËæìÂÖ•È™åËØÅÁ†Å')\r\n        this.refreshCode()\r\n        return\r\n      }\r\n      if (this.code.toLowerCase() !== this.identifyCode.toLowerCase()) {\r\n        this.$message.warning('È™åËØÅÁ†ÅÈîôËØØ')\r\n        this.refreshCode()\r\n        return\r\n      }\r\n      this.$refs['userForm'].validate((valid) => {\r\n        if (valid) {  // Ë°®ÂçïÊ†°È™åÂêàÊ≥ï\r\n          this.request.post(\"/user/login\", this.user).then(res => {\r\n            if(res.code === '200') {\r\n              localStorage.setItem(\"user\", JSON.stringify(res.data))  // Â≠òÂÇ®Áî®Êà∑‰ø°ÊÅØÂà∞ÊµèËßàÂô®\r\n              localStorage.setItem(\"menus\", JSON.stringify(res.data.menus))  // Â≠òÂÇ®Áî®Êà∑‰ø°ÊÅØÂà∞ÊµèËßàÂô®\r\n\r\n              // Âä®ÊÄÅËÆæÁΩÆÂΩìÂâçÁî®Êà∑ÁöÑË∑ØÁî±\r\n              setRoutes()\r\n              if (res.data.role === 'ROLE_USER') {\r\n                this.$router.push(\"/front/home\")\r\n                this.$message.success(\"ÁôªÂΩïÊàêÂäü\")\r\n              } else {\r\n                this.$router.push(\"/\")\r\n                this.$message.success(\"ÁôªÂΩïÊàêÂäü\")\r\n              }\r\n            } else {\r\n              this.$message.error(res.msg)\r\n            }\r\n          })\r\n        }\r\n      });\r\n    },\r\n    loginPhone() {\r\n      if (!this.code) {\r\n        this.$message.warning('ËØ∑ËæìÂÖ•È™åËØÅÁ†Å')\r\n        this.refreshCode()\r\n        return\r\n      }\r\n      if (this.code.toLowerCase() !== this.identifyCode.toLowerCase()) {\r\n        this.$message.warning('È™åËØÅÁ†ÅÈîôËØØ')\r\n        this.refreshCode()\r\n        return\r\n      }\r\n      this.$refs['userForm'].validate((valid) => {\r\n        if (valid) {  // Ë°®ÂçïÊ†°È™åÂêàÊ≥ï\r\n          this.request.post(\"/user/loginPhone\", this.user).then(res => {\r\n            if(res.code === '200') {\r\n              localStorage.setItem(\"user\", JSON.stringify(res.data))  // Â≠òÂÇ®Áî®Êà∑‰ø°ÊÅØÂà∞ÊµèËßàÂô®\r\n              localStorage.setItem(\"menus\", JSON.stringify(res.data.menus))  // Â≠òÂÇ®Áî®Êà∑‰ø°ÊÅØÂà∞ÊµèËßàÂô®\r\n\r\n              // Âä®ÊÄÅËÆæÁΩÆÂΩìÂâçÁî®Êà∑ÁöÑË∑ØÁî±\r\n              setRoutes()\r\n              if (res.data.role === 'ROLE_USER') {\r\n                this.$router.push(\"/front/home\")\r\n                this.$message.success(\"ÁôªÂΩïÊàêÂäü\")\r\n              } else {\r\n                this.$router.push(\"/\")\r\n                this.$message.success(\"ÁôªÂΩïÊàêÂäü\")\r\n              }\r\n            } else {\r\n              this.$message.error(res.msg)\r\n            }\r\n          })\r\n        }\r\n      });\r\n    },\r\n    // ÂàáÊç¢È™åËØÅÁ†Å\r\n    refreshCode() {\r\n      this.identifyCode = ''\r\n      this.makeCode(this.identifyCodes, 4)\r\n    },\r\n    // ÁîüÊàêÈöèÊú∫È™åËØÅÁ†Å\r\n    makeCode(o, l) {\r\n      for (let i = 0; i < l; i++) {\r\n        this.identifyCode += this.identifyCodes[Math.floor(Math.random() * (this.identifyCodes.length))]\r\n      }\r\n    },\r\n    handlePass() {\r\n      this.dialogFormVisible = true\r\n      this.pass = {}\r\n    },\r\n    passwordBack() {\r\n      this.request.put(\"/user/reset\", this.pass).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"ÈáçÁΩÆÂØÜÁ†ÅÊàêÂäüÔºåÊñ∞ÂØÜÁ†Å‰∏∫Ôºö123ÔºåËØ∑Â∞ΩÂø´‰øÆÊîπÂØÜÁ†Å\")\r\n          this.dialogFormVisible = false\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.wrapper {\r\n  height: 100vh;\r\n  background-image: url(\"../assets/bg.jpg\");\r\n  background-size: cover;\r\n  overflow: hidden;\r\n}\r\n</style>\r\n\r\n\r\n<!--<template>-->\r\n<!--  <div class=\"wrapper\">-->\r\n<!--    <div style=\"height: 60px; line-height: 60px; font-size: 20px; padding-left: 50px; color: mediumseagreen;-->\r\n<!--      background-color: rgba(0,0,0,0.2)\">ÊóÖÊ∏∏ÂçöÂÆ¢Á≥ªÁªü</div>-->\r\n<!--    <div style=\"margin: 150px auto; background-color: rgba(0,0,0,0.2); width: 350px; height: 360px; padding: 20px; border-radius: 10px\">-->\r\n<!--      