m.id\">\r\n          <el-image\r\n              style=\"width: 100%; height: 100%\"\r\n              :src=\"item.url\"\r\n              :fit=\"fit\"\r\n              @click=\"navigateToPage(item.id)\"><!-- 添加 @click 事件处理器 -->\r\n          </el-image>\r\n        </el-carousel-item>\r\n      </el-carousel>\r\n    </div>\r\n\r\n    <div>\r\n      <!-- 导航栏 -->\r\n      <el-menu default-active=\"1\" class=\"el-menu-demo\" mode=\"horizontal\" @select=\"handleSelect\">\r\n        <el-menu-item index=\"1\">公告</el-menu-item>\r\n        <el-menu-item index=\"2\">消息</el-menu-item>\r\n      </el-menu>\r\n\r\n      <!-- 公告内容 -->\r\n      <div v-if=\"active1 === '1'\">\r\n        <el-card style=\"width: 1100px;  margin: 10px auto\" >\r\n          <div>\r\n            <el-collapse accordion v-model=\"active\">\r\n              <el-collapse-item v-for=\"(item,index) in notices\" :key=\"item.id\" :name=\"'' + index\">\r\n                <template slot=\"title\">\r\n                  <span style=\"font-size: 20px;\">{{ item.name }}</span>\r\n                  <span style=\"margin-left: 10px\">{{ item.time }}</span>\r\n                </template>\r\n                <div v-html=\"item.content\"></div>\r\n              </el-collapse-item>\r\n            </el-collapse>\r\n          </div>\r\n        </el-card>\r\n      </div>\r\n\r\n\r\n      <!-- 消息内容 -->\r\n      <div v-if=\"active1 === '2'\">\r\n        <div style=\"width:1100px;margin-top: 15px\">\r\n          <el-card>\r\n            <el-form size=\"small\" style=\"width: 100%\">\r\n              <div style=\"padding: 10px 0; margin: 0 0 0 0; border-bottom: 1px solid #ccc; color: #1E90FF; font-size: 20px\">\r\n                留言\r\n              </div>\r\n              <div style=\"\">\r\n                <el-form-item>\r\n                  <el-input type=\"textarea\" v-model=\"content\" autocomplete=\"off\" :rows=\"3\"></el-input>\r\n                </el-form-item>\r\n              </div>\r\n            </el-form>\r\n            <div style=\"margin-top: 10px;text-align: right\">\r\n              <el-button size=\"small\" type=\"success\" @click=\"submit\">提交</el-button>\r\n            </div>\r\n          </el-card>\r\n\r\n          <div style=\"padding: 10px 0; margin: 10px 0 10px 0; border-bottom: 1px solid #ccc; color: orangered; font-size: 20px\">\r\n            大家在说什么\r\n          </div>\r\n          <div v-for=\"item in tableData\" style=\"padding:5px 0\">\r\n            <el-card>\r\n              <div style=\"display: flex\">\r\n                <div style=\"width: 80px\">\r\n                  <el-avatar :size=\"50\" :src=\"item.user.avatarUrl\"></el-avatar>\r\n                </div>\r\n                <div style=\"flex: 1\">\r\n                  <div style=\"display: flex\">\r\n                    {{ item.user.nickname }} <span style=\"margin-left: 10px\">{{ item.time }}</span>\r\n                  </div>\r\n                  <div style=\"margin-top: 10px; color: #666\">{{ item.content }}</div>\r\n                </div>\r\n              </div>\r\n              <div style=\"margin-top: 10px;margin-left: 100px; width: 90%;background-color: aliceblue; padding: 10px; border-radius: 10px\" v-if=\"item.reply != null\">\r\n                <div style=\"padding:5px 0\">\r\n                  <div style=\"width: 100px;display: flex;align-items: center\">\r\n                    <span style=\"cursor: pointer;font-weight:bold\" >管理员回复</span>\r\n                  </div>\r\n                  <br>\r\n                  <span style=\"color: #666;width: 100%;\">{{item.reply}}</span>\r\n                </div>\r\n              </div>\r\n            </el-card>\r\n          </div>\r\n          <div style=\"padding: 10px 0;\">\r\n            <el-pagination\r\n                @size-change=\"handleSizeChange\"\r\n                @current-change=\"handleCurrentChange\"\r\n                :current-page=\"pageNum\"\r\n                :page-sizes=\"[2, 5, 10, 20]\"\r\n                :page-size=\"pageSize\"\r\n                layout=\"total, prev, pager, next\"\r\n                :total=\"total\">\r\n            </el-pagination>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n  </div>\r\n\r\n\r\n</template>\r\n\r\n<script>\r\n\r\n// 前端映射表\r\nconst routeMap = {\r\n  '1': 'Page1', // 假设图片的id为1，对应Page1\r\n  '2': 'Page2', // 假设图片的id为2，对应Page2\r\n  '3': 'Page3', // 假设图片的id为3，对应Page3\r\n  // ...其他映射\r\n}\r\n\r\nexport default {\r\n  name: \"Community.vue\",\r\n  data() {\r\n    return {\r\n      files: [],\r\n      pageNum: 1,\r\n      pageSize: 5,\r\n      total: 0,\r\n      tableData: [],\r\n      notices: [],\r\n      active: '0',\r\n      active1: '1', // 默认激活选项为“公告”\r\n      content: '',\r\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\r\n      types: [],\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n    this.load1()\r\n    this.request.get(\"/notice\").then(res => {\r\n      this.notices = res.data\r\n    })\r\n  },\r\n  methods: {\r\n    navigateToPage(itemId) {\r\n      const routeName = routeMap[itemId];\r\n      if (routeName) {\r\n        this.$router.push({ name: routeName });\r\n      } else {\r\n        // 处理未找到对应页面的情况\r\n        this.$router.push({ path: '/not-found' }); // 导航到404页面\r\n      }\r\n    },\r\n    handleSelect(key, index) {\r\n      if (key === '1') { // 当点击“公告”时，更新 active1 为 '1'。\r\n        this.active1 = '1';\r\n      } else if (key === '2') { // 当点击“消息”时，更新 active1 为 '2' 并显示消息输入框。\r\n        this.active1 = '2';\r\n      } else { // 其他情况，你可以根据需要添加更多的逻辑。\r\n        this.active1 = '0'; // 重置 active1 到默认值或任何其他值。\r\n    }\r\n      },\r\n\r\n    submit() {\r\n      if (this.user.username == null){\r\n        this.$message.warning(\"请登录\")\r\n        return;\r\n      }\r\n      let data = {\r\n        content: this.content,\r\n        userId: this.user.id,\r\n      }\r\n      console.log(data);\r\n      this.request.post(\"/message\", data).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"发表成功\")\r\n          this.load1()\r\n          this.content = ''\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    load1() {\r\n      this.request.get(\"/message/page\", {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data.records\r\n        this.total = res.data.total\r\n      })\r\n    },\r\n    handleSizeChange(pageSize) {\r\n      this.pageSize = pageSize\r\n      this.load1()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.pageNum = pageNum\r\n      this.load1()\r\n    },\r\n\r\n\r\n    load() {\r\n      this.request.get(\"/file/pageHome\", {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n        }\r\n      }).then(res => {\r\n        this.files = res.data.records\r\n        this.total = res.data.total\r\n      })\r\n    },\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n<style scoped>\r\n.el-menu-demo{\r\n  background-color: white;\r\n}\r\n</style>\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n"],"mappings":";AA6GA;AACA,MAAAA,QAAA;EACA;EAAA;EACA;EAAA;EACA;EACA;AACA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,KAAA;MACAC,OAAA;MACAC,QAAA;MACAC,KAAA;MACAC,SAAA;MACAC,OAAA;MACAC,MAAA;MACAC,OAAA;MAAA;MACAC,OAAA;MACAC,IAAA,EAAAC,YAAA,CAAAC,OAAA,WAAAC,IAAA,CAAAC,KAAA,CAAAH,YAAA,CAAAC,OAAA;MACAG,KAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;IACA,KAAAC,KAAA;IACA,KAAAC,OAAA,CAAAC,GAAA,YAAAC,IAAA,CAAAC,GAAA;MACA,KAAAhB,OAAA,GAAAgB,GAAA,CAAAtB,IAAA;IACA;EACA;EACAuB,OAAA;IACAC,eAAAC,MAAA;MACA,MAAAC,SAAA,GAAA5B,QAAA,CAAA2B,MAAA;MACA,IAAAC,SAAA;QACA,KAAAC,OAAA,CAAAC,IAAA;UAAA7B,IAAA,EAAA2B;QAAA;MACA;QACA;QACA,KAAAC,OAAA,CAAAC,IAAA;UAAAC,IAAA;QAAA;MACA;IACA;IACAC,aAAAC,GAAA,EAAAC,KAAA;MACA,IAAAD,GAAA;QAAA;QACA,KAAAvB,OAAA;MACA,WAAAuB,GAAA;QAAA;QACA,KAAAvB,OAAA;MACA;QAAA;QACA,KAAAA,OAAA;MACA;IACA;IAEAyB,OAAA;MACA,SAAAvB,IA